{
  "name": "j_template_nextjs-typescript_ssg",
  "version": "0.1.0",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "export-with-sitemap": "next build && next-sitemap --config next-sitemap.config.mjs && next export",
    "type-check": "tsc",
    "test": "jest --env=jsdom --verbose",
    "build-sitemap": "next-sitemap --config next-sitemap.config.mjs",
    "lint": "next lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,scss}'",
    "typedoc": "npx typedoc --readme none --cleanOutputDir true --excludeExternals false --exclude '**/*+(.test|.spec|.e2e)*+(.ts|.js)' --exclude '**/*+(.tsx|.jsx|.d.ts)' --exclude '**/*+(.hook|.hooks)*+(.tsx|.jsx|.ts|.js)' --entryPointStrategy expand --out ./tsdoc/ ./src/",
    "clean:cache": "npm cache clean --force"
  },
  "dependencies": {
    "axios": "^1.1.0",
    "classnames": "^2.3.1",
    "next": "^12.3.0",
    "ramda": "^0.28.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "^0.7.6",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@jest/types": "^28.1.3",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/dom": "^8.16.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "@types/gtag.js": "^0.0.10",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.23",
    "@types/ramda": "^0.28.15",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "cross-fetch": "^3.1.5",
    "css-loader": "^6.7.1",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.3",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom": "^28.1.3",
    "next-sitemap": "^3.1.12",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "typedoc": "^0.23.15",
    "typescript": "^4.8.4"
  }
}
